{
    "name": "msppack/laravelapp",
    "keywords": ["laravel5.4","laravelapp","social","facebook","google"],
    "description": "Package for create easly form with single sintex",
    "authors": [
        {
            "name": "Manoj Sonagra",
            "email": "manoj30808@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "dirkgroenen/Pinterest-API-PHP": "^0.2.11",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/socialite": "^3.0",
        "doctrine/dbal": "^2.5"        
    },
    "autoload": {
        "psr-4": {
            "MspPack\\LaravelApp\\": "packages/MspPack/laravelapp/src"
        }
    },
     "scripts": {
        "post-create-project-cmd": [
            "php artisan migrate",
            "php artisan vendor:publish"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    }
}
